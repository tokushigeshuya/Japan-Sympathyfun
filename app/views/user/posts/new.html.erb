<<h1>画像投稿フォーム</h1>
  <%= form_with model: @post, url: user_posts_path, method: :post, local: true do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <h4>body</h4>
      <%= f.text_area :body %>
      <%= f.label :住所 %>
      <%= f.text_area :address, autofocus: true %>
    <%= f.submit '投稿' %>
  <% end %>
